<Window x:Class="RasterPaint.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RasterPaint"
        mc:Ignorable="d"
        Title="RasterPaint.NET" Height="250" Width="450"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5" Orientation="Horizontal">
            <Image Source="{StaticResource Polygons}" Height="192" Width="192" Margin="15" Stretch="UniformToFill"
                   MouseLeftButtonDown="MainProgram_Click"/>
            <Image Source="{StaticResource LennaImage}" Height="192" Width="192" Margin="15"
                   MouseLeftButtonDown="ColorReduction_Click"/>
        </StackPanel>    
    </Grid>
</Window>
